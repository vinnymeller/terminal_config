syntax enable
set background=dark
colorscheme solarized

set tabstop=4

au BufRead,BufNewFile *.py set expandtab
au BufRead,BufNewFile Makefile* set noexpandtab


set expandtab
set tabstop=4
set softtabstop=4
set shiftwidth=4
set autoindent

set backspace=indent,eol,start

set number
set showcmd

imap jk <Esc>
